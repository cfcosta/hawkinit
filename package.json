    "clui": "^0.3.1",
    "command-exists": "^1.0.2",
    "command-line-args": "^3.0.4",
    "command-line-usage": "^3.0.8",
    "figlet": "^1.2.0",
    "inquirer": "^1.2.3",
    "lodash": "^4.17.2",
    "mustache": "^2.3.0",
    "tmp": "0.0.31"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^11.0.0",
